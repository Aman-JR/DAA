#include<iostream>
#include<math.h>
using namespace std;

int binary(int a[],int size,int key,int &count)
{
    
    int stpoint = 0;
    int endpoint = size;
    while(stpoint <= endpoint)
    {
        count++;
        int mid = (stpoint + endpoint)/2;
        if(a[mid] == key )
        {
            return 1 + mid;
        }

        else if(a[mid]>key)
        {
            endpoint = mid-1;
        }
        else if(a[mid]<key)
        {
            stpoint = mid+1;
        }
    }
    return -1;
}
int main()
{
    int count = 0;
    int n,key;

    cin>>n;
    int *p = new int[n];

    for(int i=0;i<n;i++)
    {
        cin>>p[i];
    }

    for(int i =0;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(p[i]>p[j])
            {
                int temp =p[i];
                p[i]=p[j];
                p[j]= temp;
            }
        }
    }
    for(int i=0;i<n;i++)
    {
        cout<<p[i]<<" ";
    }
    cout<<endl;
    cin>>key;

    if(binary(p,n,key,count) == -1)
    {
        cout<<"NOT PRESENT "<<count <<endl;
    }
    else{
        cout<<"PRESENT "<<binary(p,n,key,count)<<endl;
    }

}
